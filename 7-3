#include<stdio.h>
#include<math.h>
int main(){
	int n;
	scanf("%d",&n);
	int a[n];
	int sum=0;
	for(int i=0;i<n;i++){
		scanf("%d",&a[i]);
		sum+=a[i];
	}
	double avg=(double)sum/(double)n;
	double ans=0;
	for(int i=0;i<n;i++){
		ans+=(a[i]-avg)*(a[i]-avg);
	}
	ans/=n;
	ans=sqrt(ans);
	printf("%.5lf",ans);
	return 0;
}
