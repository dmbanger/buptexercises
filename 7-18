#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int see(int a){
	if(a%3==1&&a%5==2){
		return 1;
	}else{
		return 0;
	}
}
int main(){
	int n;
	int num;
	long long sum;
	scanf("%d",&n);
	for(int i=0;i<n;i++){
		sum=0;
		while(1){
			scanf("%d",&num);
			if(num==-1){
				break;
			}else{
				if(see(num)==1){
					sum+=num;
				}
			}
		}
		printf("%lld\n",sum);
	}
	return 0;
}
