#include<stdio.h>
#include<string.h>
int main()
{
    char a[100];
    fgets(a,100,stdin);
    int i,j,k;
    int num;
    int sum=0;
    for(i=strlen(a)-2;i>=0;i--){
        j=strlen(a)-i-2;
        num=a[i]-'a';
        //printf("%d",num);
        for(k=0;k<j;k++){
          num*=17;  
        }
        sum+=num;
    }
    printf("%d",sum);
    return 0;
}
