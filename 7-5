#include<stdio.h>
int main(){
	int a,b;
	char sym;
	scanf("%d %c %d",&a,&sym,&b);
	if(sym=='+'){
		printf("%d %c %d = %d",a,sym,b,a+b);
	} else if(sym=='-'){
		printf("%d %c %d = %d",a,sym,b,a-b); 
	}else if(sym=='*'){
		printf("%d %c %d = %d",a,sym,b,a*b);
	}else if(b==0){
		printf("Invalid");
	}else if(sym=='/'){
		printf("%d %c %d = %d",a,sym,b,a/b);
	}else if(sym=='%'){
		printf("%d %c %d = %d",a,sym,b,a%b);
	}else{
		printf("Invalid"); 
	}
	return 0;
} 
