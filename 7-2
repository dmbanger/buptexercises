#include<stdio.h>
int main(){
	int m,n;
	scanf("%d %d",&m,&n);
	int classr[n][m];
	int flag=0;
	int stnum=1;
	for(int i=0;i<n;i++){
		if((i+1)%2==1){
			if(flag==0){
				for(int j=m-1;j>=0;j--){
					classr[i][j]=stnum;
					stnum++;
				}
				flag=1;
			}else if(flag==1){
				for(int j=0;j<=m-1;j++){
					classr[i][j]=stnum;
					stnum++;
				}
				flag=0;
			}
		}else{
			for(int k=0;k<m;k++){
				classr[i][k]=-1; 
			}
		}
	}
	for(int i=0;i<m;i++){
		for(int j=0;j<n;j++){
			if(classr[j][i]==-1){
				if(j!=n-1){
					printf(". ");
				}else{
					printf(".");
				}
			}else if(j!=n-1){
				printf("%d ",classr[j][i]);
			}else{
				printf("%d",classr[j][i]);
			}
		}
		printf("\n");
	}
	return 0;
}
